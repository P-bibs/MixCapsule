{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://mixcapsule.paulbiberstein.me",
  "scripts": {
    "build:tailwind": "tailwind build styles/tailwind.css -o styles/tailwind.output.css",
    "watch:tailwind": "chokidar 'styles/**/*.css' --ignore styles/tailwind.output.css -c 'npm run build:tailwind'",
    "dev": "npm-run-all build:tailwind --parallel watch:tailwind dev:next",
    "dev:next": "next dev",
    "prebuild": "NODE_ENV=production run-s build:tailwind",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "chokidar-cli": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "next": "9.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-icons": "^3.10.0",
    "react-toastify": "^6.0.8",
    "spotify-web-api-js": "^1.5.0",
    "tailwindcss": "^1.6.1",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^2.0.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
